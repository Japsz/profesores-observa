<div class="row">
  <div class="col-md-12" style="margin: 6px">
    <!-- Inscriptions -->
    <h3 class="m-0 font-weight-bold text-primary">Propuestas de Eventos</h3>
  </div>
  <%
  if(data.length > 0){
    for(var i=0; i < data.length; i++){
  %>
  <div class="col-md-4" style="padding: 6px" id="proposal_<%= data[i].idevent%>">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title"><%= data[i].title%></h4>
        <h6 class="card-subtitle mb-2 text-muted">Propuesto por: <%=data[i].name%> (<%= data[i].mail%>)</h6>
        <p class="card-text"><%= data[i].description%></p>
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item">
          <div class="float-left">Desde</div>
          <div class="float-right">Hasta</div>
        </li>
        <li class="list-group-item">
          <div class="float-left">
            <%=data[i].desde%>
          </div>
          <div class="float-right">
            <%=data[i].hasta%>
          </div>
        </li>
      </ul>
      <div class="card-body">
        <div class="link-group float-right">
          <a href="#" onclick="ajax_send(this,'','custom',{},function (nada){$('#proposal_<%= data[i].idevent%>').remove();})" data-url="/administrador/validateProposal" data-newType="1" data-idevent="<%= data[i].idevent%>" class="card-link text-success">Aprobar</a>
          <a href="#rejectProposal" onclick="$('#rejectIdEvnt').val(<%= data[i].idevent%>)" data-toggle="modal" class="card-link text-danger">Rechazar</a>
        </div>
      </div>
    </div>
  </div>
    <%
    }
  } else {
  %>
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">No hay Eventos sin aprobar</h5>
      </div>
    </div>
  </div>
  <%}%>
</div>