<div class="row">
  <!-- Inscriptions -->
  <h3 class="m-0 font-weight-bold text-primary page-header">Propuestas de Eventos</h3>
  <%
  if(data.length > 0){
    for(var i=0; i < data.length; i++){
  %>
  <div class="col-md-12" style="margin: 6px">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title"><%= data[i].title%></h4>
        <h6 class="card-subtitle mb-2 text-muted">Propuesto por: <%=data[i].name%> (<%= data[i].mail%>)</h6>
        <p class="card-text"><%= data[i].description%></p>
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item">Fecha de inicio: <%= new Date(data[i].start).toLocaleString()%></li>
        <li class="list-group-item">Fecha de TÃ©rmino: <%= new Date(data[i].end).toLocaleString()%></li>
      </ul>
      <div class="card-body">
        <a href="#" class="card-link text-success">Aprobar</a>
        <a href="#" class="card-link text-danger">Denegar</a>
      </div>
    </div>
  </div>
    <%
    }
  } else {
  %>
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">No hay Eventos sin aprobar</h5>
      </div>
    </div>
  </div>
  <%}%>
</div>